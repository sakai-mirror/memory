<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

   <!-- test registering and running beans -->
   <bean class="org.sakaiproject.testrunner.util.TestExecutor">
      <property name="testClassname"
         value="org.sakaiproject.memory.test.LoadTestMemoryService" />
      <property name="testType" value="testrunner.load" />
      <property name="registerTest" value="true" />
      <property name="testDelaySeconds" value="35" />
   </bean>

</beans>
